    "gotrue",
    "auth",
    "jwt",
    "typescript",
    "ogna"
  ],
  "author": "Santiago Velez",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^7.0.0",
    "typescript": "^5.0.0"
  }
